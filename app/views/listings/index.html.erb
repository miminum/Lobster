<p id="notice"><%= notice %></p>

<h1>Listings</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Open</th>
      <th>Close</th>
      <th>Item</th>
      <th>Quantity</th>
      <th>Seller profile</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @listings.each do |listing| %>
      <tr>
        <td><%= listing.date %></td>
        <td><%= listing.open %></td>
        <td><%= listing.close %></td>
        <td><%= listing.item %></td>
        <td><%= listing.quantity %></td>
        <td><%= listing.seller_profile %></td>
        <td><%= link_to 'Show', listing %></td>
        <td><%= link_to 'Edit', edit_listing_path(listing) %></td>
        <td><%= link_to 'Destroy', listing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Listing', new_listing_path %>
